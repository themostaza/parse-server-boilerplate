{
  "extends": "airbnb-base",
  "globals": {
    "Parse": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
  },
  "parser": "babel-eslint",
  "rules": {
    "arrow-parens": 0,
    "comma-dangle": [2, "never"],
    "new-cap": [0],
    "no-console": [0],
    "no-param-reassign": [1],
    "no-use-before-define": [0],
    "semi": [2, "never"]
  }
}
